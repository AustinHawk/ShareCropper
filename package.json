{
  "name": "ShareCropper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently -k \"nodemon ./app.js\" \"npm run watch\"",
    "watch": "watchify client/index.js -o public/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "postinstall": "browserify client/index.js -o public/bundle.js -t [ babelify --presets [ es2015 react ] ]"
  },
  "dependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.1",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-fileupload": "0.0.5",
    "express-session": "^1.14.0",
    "hbs": "~3.1.0",
    "mongoose": "^4.5.5",
    "mongoose-findorcreate": "^0.1.2",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "s3": "^4.4.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "watchify": "^3.7.0"
  }
}
